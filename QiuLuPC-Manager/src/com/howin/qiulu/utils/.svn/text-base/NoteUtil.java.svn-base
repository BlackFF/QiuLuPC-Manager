package com.howin.qiulu.utils;

import com.howin.qiulu.pojo.Category;
import com.howin.qiulu.pojo.custom.UItreeNote;

public class NoteUtil {

	public static UItreeNote getNote(Category category){
		UItreeNote uItreeNote = new UItreeNote();
		uItreeNote.setId(category.getId());
		uItreeNote.setText(category.getName());
		return uItreeNote;
	}
}
