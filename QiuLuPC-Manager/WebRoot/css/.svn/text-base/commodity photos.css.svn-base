.photo{
	width: 100%;
}
.photo ul li{
	position: relative;
	text-align: center;
	width: 250px;
	background: url(../images/del.jpg) no-repeat top right;
	padding: 10px;
	float: left;
	-webkit-user-select:none ;
	user-select:none ;
}
.photo ul li a{
	position: absolute;
	top: 0;
	right: 0;
	width: 20px;
}
.photo ul li img{
	height: 150px;
	width: 200px;
	margin-bottom: 10px;
}
